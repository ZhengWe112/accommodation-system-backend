<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mybatisplus.mapper.MaintenanceAdministratorMapper">
    <select id="selectByManager1" resultType="com.example.mybatisplus.model.domain.MaintenanceAdministrator">
        select  * from maintenance_administrator
        <where>
            <if test="maintenance_administrator.job_id != null">
                and job_id = #{maintenance_administrator.job_id}
            </if>
            <if test="maintenance_administrator.fullname != null and maintenance_administrator.fullname != ''">
                and fullname= #{maintenance_administrator.fullname}
            </if>
            <if test="maintenance_administrator.password != null">
                and password = #{maintenance_administrator.password}
            </if>
        </where>
    </select>
    <select id="selectByManager3" resultType="com.example.mybatisplus.model.domain.ResponsibleLeader">
        select  * from responsible_leader
        <where>
            <if test="responsible_leader.job_id != null">
                and job_id = #{responsible_leader.job_id}
            </if>
            <if test="responsible_leader.fullname != null and responsible_leader.fullname != ''">
                and fullname= #{responsible_leader.fullname}
            </if>
            <if test="responsible_leader.password != null">
                and password = #{responsible_leader.password}
            </if>
        </where>
    </select>
    <select id="selectByManager2" resultType="com.example.mybatisplus.model.domain.SystemAdministrator">
        select  * from system_administrator
        <where>
            <if test="system_administrator.job_id != null">
                and job_id = #{system_administrator.job_id}
            </if>
            <if test="system_administrator.fullname != null and system_administrator.fullname != ''">
                and fullname= #{maintenance_administrator.fullname}
            </if>
            <if test="system_administrator.password != null">
                and password = #{system_administrator.password}
            </if>
        </where>
    </select>
</mapper>
