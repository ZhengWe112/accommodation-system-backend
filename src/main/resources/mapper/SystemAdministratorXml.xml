<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mybatisplus.mapper.SystemAdministratorMapper">
    <select id="selectBySystemAdministrator" resultType="com.example.mybatisplus.model.domain.SystemAdministrator">
        select  * from system_administrator
        <where>
            <if test="system_administrator.job_id != null">
                and job_id = #{system_administrator.job_id}
            </if>
            <if test="system_administrator.fullname != null and system_administrator.fullname != ''">
                and fullname= #{maintenance_administrator.fullname}
            </if>
            <if test="system_administrator.password != null">
                and password = #{system_administrator.password}
            </if>
        </where>
    </select>
</mapper>
